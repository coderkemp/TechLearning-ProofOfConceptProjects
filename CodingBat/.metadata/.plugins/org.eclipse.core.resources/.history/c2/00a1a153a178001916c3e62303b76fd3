package geeks;

public class ArrayReversal {

	 public static void arrayReversal(int arr[],int sizeOfArray) 
	    {
	       int test[]=new int[28123];
	       int x=0;
	       test=arr;
	       for(int i=sizeOfArray-1;i>=0;i--){
	           arr[x]=test[i];
	           x++;
	       }
	    }
	 
	public static void main(String[] args) {
		// TODO Auto-generated method stub

	}

}
